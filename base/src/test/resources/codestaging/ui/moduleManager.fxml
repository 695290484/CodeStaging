<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TextArea?>
<HBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.zj.codestaging.ui.Controller">
    <padding>
        <Insets bottom="0.0" left="10.0" right="0.0" top="10.0" />
    </padding>
    <VBox>
        <HBox spacing="5.0">
            <VBox spacing="5.0" fx:id="leftBox" maxHeight="200" alignment="CENTER">
                <Label>未加载模块</Label>
            </VBox>
            <VBox alignment="center" spacing="3.0">
                <Button text="&gt;&gt;" fx:id="loadBtn"></Button>
                <Button text="&lt;&lt;" fx:id="unloadBtn"></Button>
            </VBox>
            <VBox spacing="5.0" fx:id="rightBox" maxHeight="200" alignment="CENTER">
                <Label>已加载模块</Label>
            </VBox>
        </HBox>
    </VBox>
</HBox>
